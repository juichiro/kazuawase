<div class='page-header'><h1>ユーザー情報の編集</h1></div>

<%= form_for(@user) do |f| %>
<div class='form-group'>
  <%= f.label :name, 'Name' %>
  <%= f.text_field :name, class:'form-control' %>
</div>
<div class='form-group'>
  <%= f.label :myself, '自己紹介' %>
  <%= f.text_area :myself, class:'form-control', size:'10x10' %>
</div>
  <%= f.submit '登録', class:'btn btn-white-orange-m' %>
<% end %>